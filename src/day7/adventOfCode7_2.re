  |> Js.String.split("\n")
  |> Array.map(
       (a) => {
         let [first, ...rest] = words(a);
         let index =
           switch (number(a)) {
           | Some(result) => int_of_string(result)
           | None => 0
           };
         (first, index, rest)
       }
     )
  |> Array.to_list;

let isLeaf = ((_, _, children)) => List.length(children) == 0;

let isNode = ((_, _, children)) => List.length(children) > 0;

let leaves = List.filter(isLeaf, input);

let nodes = List.filter(isNode, input);

let findByName = (name) =>
  input
  |> (
    switch (List.find(((nodeName, _, _)) => name == nodeName)) {
    | y => y
    | exception Not_found => exit(1)
    }
  );

let childrenToItems = ((_, _, children)) => children |> List.map((name) => findByName(name));

let containsChild = (nameToFind, (_, _, children)) =>
  List.exists((x) => nameToFind == x, children);

let findParentByName = (name) => List.find(containsChild(name), nodes);

let rec getChildWeight = (x) => {
  let (_, ownWeight, _) = x;
  let items = childrenToItems(x);
  let childrenWeight =
    items
    |> List.fold_left(
         (a, b) =>
           if (isLeaf(b)) {
             let (_, weight, _) = b;
             a + weight
           } else {
             a + getChildWeight(b)
           },
         ownWeight
       );
  childrenWeight
};

let checkChildBalance = (x) =>
  x
  |> List.map(
       (x) => {
         let item = findByName(x);
         let (name, _, children) = item;
         let omg =
           List.map((q) => getChildWeight(findByName(q)), children)
           |> List.fold_left((a, b) => a ++ "," ++ string_of_int(b), "");
         (name, getChildWeight(item), omg)
       }
     )
  |> List.sort_uniq(((_, x, _), (_, y, _)) => compare(x, y));

let rec isBalanced = (item) => {
  let (name, _, child) = item;
  /** FIXME: okay, this kinds sucks: it just tells you the mismatches, take the lowest one and go from there **/
  (
    if (name != "ahnofa") {
      let parent = findParentByName(name);
      let list = checkChildBalance(child);
      if (List.length(list) > 1) {
        Js.log(list)
      };
      isBalanced(parent)
    }
  )
};

Js.log("start");

List.iter(isBalanced, leaves);

Js.log("stop");
