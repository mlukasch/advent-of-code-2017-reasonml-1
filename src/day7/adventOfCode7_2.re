let words = (string) =>
  switch (Js.String.match([%re "/[a-zA-z]+/gi"], string)) {
  | Some(result) => Array.to_list(result)
  | None => Array.to_list([||])
  };

let number = (string) =>
  [%re "/(\\d+)/"]
  |> Js.Re.exec(string)
  |> (
    fun
    | Some(result) => Js.Nullable.to_opt(Js.Re.captures(result)[1])
    | None => None
  );

