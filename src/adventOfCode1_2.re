let captcha1 = "1212";

let captcha2 = "1221";

let captcha3 = "123425";

let captcha4 = "123123";

let captcha5 = "12131415";

/* My captcha */
let captcha6 = "";

let intFromChar = (char) => int_of_string(String.make(1, char));

let solveCapthcha = (captcha) => {
  let length = String.length(captcha);
  let half = length / 2;
  let arr = Array.make(length, (0, 0));
  for (n in 0 to length - 1) {
    let x = captcha.[n];
    let y = captcha.[(n + half) mod length];
    Js.log(intFromChar(y));
    arr[n] = (intFromChar(x), intFromChar(y))
  };
  arr
  |> Array.fold_left(
       (a, b) =>
         if (fst(b) == snd(b)) {
           a + fst(b)
         } else {
           a
         },
       0
     )
};

Js.log(solveCapthcha(captcha1));

Js.log(solveCapthcha(captcha2));

Js.log(solveCapthcha(captcha3));

Js.log(solveCapthcha(captcha4));

Js.log(solveCapthcha(captcha5));

Js.log(solveCapthcha(captcha6));
